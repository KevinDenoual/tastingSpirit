<div class="container">
    <h5>Dernier utilisateur</h5>
    <ul class="collection">
        {{#each (limitEach dbUser 3) }}
        <li class="collection-item avatar">
            <img src="{{ imgUser }}" alt="" class="circle">
            <p>{{ firstname }} </p>
            <p>{{ lastname }} </p>
            <p>{{ email }} </p>
        </li>
        {{/each }}
    </ul>
</div>









