<div class="container">
    <form action="/admin/createFiche" method="POST" enctype="multipart/form-data">
        <h3>création fiche détail</h3>
        <div class="row">
            <div class="input-field col s6">
                <input placeholder="whisky" type="text" name="whiskyName">
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6">
                <input placeholder="distillerie" type="text" name="distillerie">
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6">
                <input placeholder="type" type="text" name="type">
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6">
                <input placeholder="origine" type="text" name="origine">
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6">
                <input placeholder="finition" type="text" name="finition">
            </div>
        </div>
        <div class="row">
            <div class="input-field col s3">
                <input placeholder="titre" type="text" name="titre">
            </div>
            <div class="input-field col s3">
                <input placeholder="prix conseillé" type="text" name="prix">
            </div>
        </div>
        <div class="file-field input-field">
            <div class="btn">
                <span>Image</span>
                <input type="file" name="imgFiche">
            </div>
            <div class="file-path-wrapper">
                <input class="file-path validate" type="text">
            </div>
        </div>
        <button class="btn waves-effect waves-light" type="submit">validé
            <i class="material-icons right">send</i>
        </button>

    </form>
</div>


